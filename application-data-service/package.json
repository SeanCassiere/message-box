{
  "name": "message-box-application-data-service",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/index.js",
    "build:killDist": "rimraf ./dist",
    "build:code": "tsc && tsc-alias",
    "build": "npm run build:killDist && npm run build:code",
    "db:migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "db:migrate:undo": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "watch": "ts-node-dev --respawn -r tsconfig-paths/register src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/config": "^0.0.40",
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.12",
    "@types/pino": "^7.0.5",
    "@types/uuid": "^8.3.3",
    "module-alias": "^2.2.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "pino": "^7.9.1",
    "pino-pretty": "^7.5.4",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "_moduleAliases": {
    "#root": "./src"
  }
}
